msgstr "вывод списка всех серверов в кластере:"

msgid ""
"query {\n"
"    servers { alias uri uuid }\n"
"}"
msgstr ""
"query {\n"
"    servers { alias uri uuid }\n"
"}"

msgid "listing all replica sets with their servers:"
msgstr "вывод списка всех наборов реплик с серверами:"

msgid ""
"query {\n"
"    replicasets {\n"
"        uuid\n"
"        roles\n"
"        servers { uri uuid }\n"
"    }\n"
"}"
msgstr ""
"query {\n"
"    replicasets {\n"
"        uuid\n"
"        roles\n"
"        servers { uri uuid }\n"
"    }\n"
